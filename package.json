    "@solana/web3.js": "^1.98.0",
    "axios": "^1.8.4",
    "bn.js": "^5.2.1",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.5",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "typescript": "^5.3.3"
  }
